{
    "name": "Battery Life",
    "description": "Get Notified once your device hits a certain percentage to prevent destroying your battery",
    "version": "6.9",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["notifications"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/Assets/16.png",
          "32": "/Assets/32.png",
          "48": "/Assets/48.png",
          "128": "/Assets/128.png"
        }
    },
    "icons": {
        "16": "/Assets/16.png",
        "32": "/Assets/32.png",
        "48": "/Assets/48.png",
        "128": "/Assets/128.png"
      }
}